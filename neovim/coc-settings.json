{
  "diagnostic.warningSign": "!",
  "diagnostic.errorSign": "X",
  "diagnostic.infoSign": "i",
  "diagnostic.hintSign": "?",
  "python.formatting.provider": "black",
  "tsserver.locale": "ja",
  "css.lint.unknownAtRules": "ignore",
  "rpc.idleText": "にゃーん",
  "rpc.lowerDetailsEditing": "@ {workspace_folder}",
  "rpc.largeImage": "言語：{LANG}",
  "rpc.detailsIdling": "(-ω-) なにもしていない",
  "rpc.detailsEditing": "{file_name} を編集中",
  "rpc.lowerDetailsNotFound": "どこか",
  "rpc.detailsViewing": "{file_name} を閲覧中",
  "rpc.lowerDetailsIdling": "にゃーん",
  "rpc.lowerDetailsViewing": "@ {workspace_folder}",
  "rpc.workspaceElapsedTime": true,
  "tsserver.log": "verbose",
  "tsserver.trace.server": "verbose",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "sumneko-lua.enableNvimLuaDev": true,
  "diagnostic.virtualText": true,
  "powershell.integratedConsole.showOnStartup": false,
  "solargraph.diagnostics": true,
  "solargraph.formatting": false,
  "typescript.format.enable": false,
  "scss.lint.unknownAtRules": "ignore",
  "languageserver": {
    "steep": {
      "command": "steep",
      "args": ["langserver", "--jobs", "2"],
      "filetypes": ["ruby", "rbs"],
      "rootPatterns": ["Steepfile"]
    },
    "ruby_syntax_lsp": {
      "command": "ruby_syntax_lsp",
      "args": [],
      "filetypes": ["ruby"],
      "rootPatterns": ["Gemfile"]
    },
    "ruby-lsp": {
      "command": "ruby-lsp",
      "args": [],
      "filetypes": ["ruby"],
      "rootPatterns": ["Gemfile"]
    },
    "syntax_tree": {
      "command": "stree",
      "args": ["lsp", "--plugins", "rbs"],
      "filetypes": ["ruby", "rbs"],
      "rootPatterns": ["Gemfile"]
    }
  },
  "rust-analyzer.server.extraEnv": {
    "FFMPEG_DIR": "C:/tools/ffmpeg"
  },
  "rust-analyzer.checkOnSave.command": "clippy",
  "snippets.ultisnips.pythonPrompt": false,
  "svelte.ask-to-enable-ts-plugin": false
}
